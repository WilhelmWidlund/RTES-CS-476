#include <stdio.h>

/*
 *--------------------------------- THIS IS CPU 1 ---------------------------------------------
 */

void part1()
{
	printf("Hello from the multicore system, this is CPU %d talking! \n", ALT_CPU_CPU_ID_VALUE);
}

